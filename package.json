{
  "name": "common-tags",
  "description": "a few common utility template tags for ES2015",
  "version": "0.0.1",
  "keywords": [
    "es2015",
    "es6",
    "babel",
    "template",
    "tag",
    "tagged",
    "literal",
    "string",
    "strings",
    "multi",
    "single",
    "one",
    "line",
    "multiline",
    "singleline",
    "oneline",
    "es6-tag",
    "es2015-tag",
    "strip",
    "indent",
    "indents",
    "normalize",
    "array"
  ],
  "scripts": {
    "release": "npm publish",
    "prerelease": "mv lib src && babel src -d lib --stage 0",
    "postrelease": "rm -rf lib && mv src lib",
    "test": "mocha",
    "coverage": "babel-node ./node_modules/.bin/isparta cover _mocha",
    "coveralls": "coveralls < coverage/lcov.info",
    "precoveralls": "npm run coverage"
  },
  "main": "lib",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/declandewet/common-tags"
  },
  "bugs": {
    "url": "http://github.com/declandewet/common-tags/issues",
    "email": "declandewet@me.com"
  },
  "directories": {
    "lib": "lib"
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "chai": "^3.4.0",
    "coveralls": "^2.11.4",
    "isparta": "^4.0.0",
    "mocha": "^2.3.3"
  }
}
